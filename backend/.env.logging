# Minimal Logging Configuration
# Copy these to your .env file to reduce logging

# Set log level to only show errors
LOG_LEVEL=error

# Disable verbose logging
ENABLE_PERFORMANCE_LOGGING=false
ENABLE_REQUEST_LOGGING=false
ENABLE_RESPONSE_LOGGING=false
ENABLE_SYSTEM_STATE_LOGGING=false
ENABLE_STRUCTURED_LOGGING=false
ENABLE_SLOW_REQUEST_LOGGING=false
ENABLE_ERROR_CONTEXT_LOGGING=false

# Set to production to reduce Prisma query logging
NODE_ENV=production